{
  "name": "feature-template",
  "version": "1.0.0",
  "description": "Template for building features using shared-agents library",
  "main": "src/index.ts",
  "scripts": {
    "agent:research": "ts-node scripts/01-run-user-research.ts",
    "agent:prd": "ts-node scripts/02-run-prd.ts",
    "agent:design": "ts-node scripts/03-run-product-designer.ts",
    "agent:architecture": "ts-node scripts/04-run-tech-architecture.ts",
    "agent:pipeline": "npm run agent:research && npm run agent:prd && npm run agent:design && npm run agent:architecture",
    "build": "tsc && npm run build:frontend && npm run build:backend",
    "build:frontend": "cd src/frontend && npm run build",
    "build:backend": "cd src/backend && npm run build",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:backend": "cd src/backend && npm run dev",
    "deploy:staging": "npm run build && npm run deploy -- --env=staging",
    "deploy:production": "npm run build && npm run deploy -- --env=production",
    "deploy": "echo 'Deploying...' && echo 'Configure your deployment command here'",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}"
  },
  "keywords": [
    "feature-template",
    "shared-agents",
    "ai-agents",
    "product-development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@your-org/shared-agents": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
